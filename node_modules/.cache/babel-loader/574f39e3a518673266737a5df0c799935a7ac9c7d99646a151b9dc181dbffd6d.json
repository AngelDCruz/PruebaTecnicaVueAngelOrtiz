{"ast":null,"code":"import ruletaDatos from \"@/components/ruleta-datos.vue\";\nexport default {\n  name: \"ruleta-juego\",\n  data() {\n    return {\n      datosEmitidos: {\n        color: 'S/A',\n        numero: 0\n      },\n      colores: [\"C34A36\", \"4B4453\"]\n    };\n  },\n  components: {\n    ruletaDatos\n  },\n  methods: {\n    datosEmitidosEvento(datos) {\n      this.datosEmitidos = datos;\n      let numGanador = this.generarNumAleatorio();\n      let colorGanador = this.generarNumAleatorioParaColores();\n      console.log(colorGanador);\n      if (numGanador == datos.numero) alert(\"Ganador\");\n      if (colorGanador == datos.color) alert(\"Ganador\");\n    },\n    // generarNumeroAleatorio(){\n    //   this.numeroGanador =\n    // }\n    generarNumAleatorio() {\n      return Math.ceil(Math.random() * 5);\n    },\n    generarNumAleatorioParaColores() {\n      let numAleatorio = Math.ceil(Math.random() * 2);\n      return this.colores[numAleatorio - 1];\n    }\n  },\n  computed: {}\n};","map":{"version":3,"names":["ruletaDatos","name","data","datosEmitidos","color","numero","colores","components","methods","datosEmitidosEvento","datos","numGanador","generarNumAleatorio","colorGanador","generarNumAleatorioParaColores","console","log","alert","Math","ceil","random","numAleatorio","computed"],"sources":["src/views/ruleta-juego.vue"],"sourcesContent":["<template>\r\n  <v-row dense justify=\"center\" align=\"start\">\r\n    <v-col cols=\"12\" class=\"titulo\">\r\n      <h2 class=\"text-center white--text pa-5\">\r\n        Juego de ruleta {{ datosEmitidos.color }} -\r\n        {{ datosEmitidos.numero }}\r\n      </h2>\r\n    </v-col>\r\n    <v-col cols=\"8\">\r\n      <ruletaDatos @emitirDatos=\"datosEmitidosEvento\" />\r\n    </v-col>\r\n  </v-row>\r\n</template>\r\n\r\n<script>\r\nimport ruletaDatos from \"@/components/ruleta-datos.vue\";\r\n\r\nexport default {\r\n  name: \"ruleta-juego\",\r\n\r\n  data() {\r\n    return {\r\n      datosEmitidos: {\r\n        color: 'S/A',\r\n        numero: 0\r\n      },\r\n      colores: [\"C34A36\", \"4B4453\"],\r\n    };\r\n  },\r\n\r\n  components: {\r\n    ruletaDatos,\r\n  },\r\n\r\n  methods: {\r\n    datosEmitidosEvento(datos) {\r\n      this.datosEmitidos = datos;\r\n      let numGanador = this.generarNumAleatorio();\r\n      let colorGanador = this.generarNumAleatorioParaColores();\r\n\r\n      console.log(colorGanador);\r\n      if (numGanador == datos.numero) alert(\"Ganador\");\r\n      if (colorGanador == datos.color) alert(\"Ganador\");\r\n    },\r\n    // generarNumeroAleatorio(){\r\n    //   this.numeroGanador =\r\n    // }\r\n    generarNumAleatorio() {\r\n      return Math.ceil(Math.random() * 5);\r\n    },\r\n    generarNumAleatorioParaColores() {\r\n      let numAleatorio = Math.ceil(Math.random() * 2);\r\n      return this.colores[numAleatorio - 1];\r\n    },\r\n  },\r\n  computed: {},\r\n};\r\n</script>\r\n\r\n<style>\r\n.contenedor {\r\n  background: #f6f6f6 !important;\r\n}\r\n.titulo {\r\n  background: rgba(10, 92, 179, 0.977);\r\n}\r\n</style>\r\n"],"mappings":"AAeA,OAAAA,WAAA;AAEA;EACAC,IAAA;EAEAC,KAAA;IACA;MACAC,aAAA;QACAC,KAAA;QACAC,MAAA;MACA;MACAC,OAAA;IACA;EACA;EAEAC,UAAA;IACAP;EACA;EAEAQ,OAAA;IACAC,oBAAAC,KAAA;MACA,KAAAP,aAAA,GAAAO,KAAA;MACA,IAAAC,UAAA,QAAAC,mBAAA;MACA,IAAAC,YAAA,QAAAC,8BAAA;MAEAC,OAAA,CAAAC,GAAA,CAAAH,YAAA;MACA,IAAAF,UAAA,IAAAD,KAAA,CAAAL,MAAA,EAAAY,KAAA;MACA,IAAAJ,YAAA,IAAAH,KAAA,CAAAN,KAAA,EAAAa,KAAA;IACA;IACA;IACA;IACA;IACAL,oBAAA;MACA,OAAAM,IAAA,CAAAC,IAAA,CAAAD,IAAA,CAAAE,MAAA;IACA;IACAN,+BAAA;MACA,IAAAO,YAAA,GAAAH,IAAA,CAAAC,IAAA,CAAAD,IAAA,CAAAE,MAAA;MACA,YAAAd,OAAA,CAAAe,YAAA;IACA;EACA;EACAC,QAAA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}