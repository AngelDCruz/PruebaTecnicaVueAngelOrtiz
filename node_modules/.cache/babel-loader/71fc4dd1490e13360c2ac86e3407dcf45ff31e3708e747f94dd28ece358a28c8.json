],\r\n    };\r\n  },\r\n\r\n  components: {\r\n    ruletaDatos,\r\n  },\r\n\r\n  methods: {\r\n    datosEmitidosEvento(datos) {\r\n      this.datosEmitidos = datos;\r\n      let numGanador = this.generarNumAleatorio();\r\n      let colorGanador = this.generarNumAleatorioParaColores();\r\n\r\n      console.log(colorGanador);\r\n      if (numGanador == datos.numero) alert(\"Ganador\");\r\n      if (colorGanador == datos.color) alert(\"Ganador\");\r\n    },\r\n    // generarNumeroAleatorio(){\r\n    //   this.numeroGanador =\r\n    // }\r\n    generarNumAleatorio() {\r\n      return Math.ceil(Math.random() * 5);\r\n    },\r\n    generarNumAleatorioParaColores() {\r\n      let numAleatorio = Math.ceil(Math.random() * 2);\r\n      return this.colores[numAleatorio - 1];\r\n    },\r\n  },\r\n  computed: {},\r\n};\r\n</script>\r\n\r\n<style>\r\n.contenedor {\r\n  background: #f6f6f6 !important;\r\n}\r\n.titulo {\r\n  background: rgba(10, 92, 179, 0.977);\r\n}\r\n</style>\r\n"],"mappings":"AAeA,OAAAA,WAAA;AAEA;EACAC,IAAA;EAEAC,KAAA;IACA;MACAC,aAAA;MACAC,OAAA;IACA;EACA;EAEAC,UAAA;IACAL;EACA;EAEAM,OAAA;IACAC,oBAAAC,KAAA;MACA,KAAAL,aAAA,GAAAK,KAAA;MACA,IAAAC,UAAA,QAAAC,mBAAA;MACA,IAAAC,YAAA,QAAAC,8BAAA;MAEAC,OAAA,CAAAC,GAAA,CAAAH,YAAA;MACA,IAAAF,UAAA,IAAAD,KAAA,CAAAO,MAAA,EAAAC,KAAA;MACA,IAAAL,YAAA,IAAAH,KAAA,CAAAS,KAAA,EAAAD,KAAA;IACA;IACA;IACA;IACA;IACAN,oBAAA;MACA,OAAAQ,IAAA,CAAAC,IAAA,CAAAD,IAAA,CAAAE,MAAA;IACA;IACAR,+BAAA;MACA,IAAAS,YAAA,GAAAH,IAAA,CAAAC,IAAA,CAAAD,IAAA,CAAAE,MAAA;MACA,YAAAhB,OAAA,CAAAiB,YAAA;IACA;EACA;EACAC,QAAA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}