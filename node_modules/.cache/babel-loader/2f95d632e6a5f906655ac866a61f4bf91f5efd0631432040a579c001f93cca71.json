.datosGanadores.esPar !== null && !this.datosGanadores.color) {\n        alert(\"numero, impar\");\n      } else if (this.datosGanadores.esPar == null && this.datosGanadores.numero && this.datosGanadores.color) {\n        alert(\"numero y color ganadores\");\n      } else if (!this.datosGanadores.color && this.datosGanadores.esPar == null && this.datosGanadores.numero) {\n        alert(\"numero ganador\");\n        this.nuevoMonto = this.datosEmitidos.monto * 2;\n      } else if (!this.datosGanadores.numero && this.datosGanadores.esPar == null && this.datosGanadores.color) {\n        alert(\"color ganador\");\n      } else if (!this.datosGanadores.numero && !this.datosGanadores.color && this.datosGanadores.esPar != null) {\n        alert(`el numero ganador es ${this.datosGanadores.esPar ? \"Par\" : \"Impar\"}`);\n      } else if (!this.datosGanadores.numero && this.datosGanadores.color && this.datosGanadores.esPar != null) {\n        alert(`color ganador y numero ${this.datosGanadores.esPar ? \"Par\" : \"Impar\"}`);\n      } else {\n        ///\n      }\n    },\n    esPar(numero) {\n      return numero % 2 == 0;\n    },\n    calcularGrados(numero) {\n      let grados = 360 / 32;\n      return numero * grados;\n    },\n    restarMonto() {\n      this.da;\n    }\n  },\n  computed: {}\n};","map":{"version":3,"names":["ruletaDatos","ruletaJuego","name","components","data","datosEmitidos","color","numero","monto","colores","datosGanadores","esPar","datosGanadorRuleta","nuevoMonto","methods","datosEmitidosEvento","datos","numGanador","generarNumAleatorio","grados","calcularGrados","$refs","ruletaTablero","iniciarJuego","esParGanador","colorGanador","console","log","setTimeout","validarPremio","Math","ceil","random","generarNumAleatorioParaColores","numAleatorio","alert","restarMonto","da","computed"],"sources":["src/views/ruleta-juego.vue"],"sourcesContent":["<template>\r\n  <v-row dense justify=\"center\" align=\"start\">\r\n    <v-col cols=\"12\" class=\"titulo mb-8\">\